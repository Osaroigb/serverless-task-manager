{
  "name": "serverless-task-manager",
  "version": "0.1.0",
  "bin": {
    "serverless-task-manager": "bin/serverless-task-manager.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "rm -rf dist && rm -f bin/*.d.ts bin/*.js lambdas/*.d.ts lambdas/*.js lib/*.d.ts lib/*.js test/*.d.ts test/*.js",
    "sam-cleanup": "rm -rf .aws-sam/",
    "format": "prettier --write \"bin/**/*.ts\" \"lambdas/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.68.0",
    "@types/jest": "^29.5.12",
    "@types/node": "22.5.4",
    "@types/sinon": "^17.0.3",
    "aws-cdk": "2.162.1",
    "aws-sdk-mock": "^6.2.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.134.0",
    "@aws-cdk/aws-dynamodb": "^1.134.0",
    "@aws-cdk/aws-lambda": "^1.134.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.134.0",
    "@aws-cdk/core": "^1.134.0",
    "@types/aws-lambda": "^8.10.145",
    "aws-sdk": "^2.1691.0",
    "source-map-support": "^0.5.19"
  }
}
